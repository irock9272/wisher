    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "prisma": "^5.4.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.4.2",
    "@types/uuid": "^9.0.1"
  }
}
